{
  "name": "@ensemble-ai/sdk",
  "version": "0.4.0",
  "description": "TypeScript SDK for the Agentic Hub",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "npm run typechain && npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "upload-docs": "aws s3 cp ./docs s3://$ENSEMBLE_S3_BUCKET_NAME --recursive",
    "typechain": "typechain --target ethers-v6 ./src/abi/*.json --out-dir ./typechain"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "chai": "^4.3.6",
    "dotenv": "^16.4.7",
    "ethers": "^6.9.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.2",
    "jest": "^29.7.0",
    "pinata-web3": "^0.5.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@graphprotocol/client-cli": "^3.0.7",
    "@swc/jest": "^0.2.37",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/chai": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typechain": "^8.3.2",
    "typedoc": "^0.27.4",
    "typescript": "^5.3.2"
  }
}
