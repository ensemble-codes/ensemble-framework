{
  "name": "@ensemble-ai/api",
  "version": "0.1.0",
  "description": "REST API server for Ensemble Framework agent discovery and management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["fastify", "api", "rest", "agents", "blockchain"],
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.2",
    "@fastify/jwt": "^7.2.4",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/env": "^4.3.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "ethers": "^6.8.1",
    "@ensemble-ai/sdk": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "typescript": "^5.2.2",
    "tsx": "^4.1.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
